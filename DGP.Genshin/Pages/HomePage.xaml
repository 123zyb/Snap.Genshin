<Page x:Class="DGP.Genshin.Pages.HomePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:DGP.Genshin.Pages" 
      xmlns:controls="clr-namespace:DGP.Genshin.Controls"
      xmlns:ui="http://schemas.modernwpf.com/2019" 
      xmlns:c="clr-namespace:DGP.Genshin.Controls.Characters"
      mc:Ignorable="d" 
      d:DesignHeight="600" d:DesignWidth="800"
      Title="首页">
    <Page.Resources>
        <Style TargetType="{x:Type ui:GridViewItem}">
            <Setter Property="Margin" Value="4,4,8,8"/>
        </Style>
    </Page.Resources>
    <Grid Margin="12,0,0,0">
    </Grid>
</Page>
